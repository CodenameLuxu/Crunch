            android:id="@+id/action_f02_Fragment_to_f01_Fragment"
            app:destination="@id/f01_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/f03_Fragment"
        android:name="com.example.basiclogin.Application.F03_Fragment"
        android:label="F03_Fragment" >
        <action
            android:id="@+id/action_f03_Fragment_to_f01_Fragment"
            app:destination="@id/f01_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/f90_Fragment"
        android:name="com.example.basiclogin.Application.F90_Fragment"
        android:label="F90_Fragment" >
        <action
            android:id="@+id/action_f90_Fragment_to_f91_Fragment"
            app:destination="@id/f91_Fragment" />
        <action
            android:id="@+id/action_f90_Fragment_to_f01_Fragment"
            app:destination="@id/f01_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/f91_Fragment"
        android:name="com.example.basiclogin.Application.F91_Fragment"
        android:label="F91_Fragment" >
        <action
            android:id="@+id/action_f91_Fragment_to_f90_Fragment"
            app:destination="@id/f90_Fragment" />
    </fragment>
</navigation>