<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/F90_Btn_TimePeriod"
        android:layout_width="188dp"
        android:layout_height="70dp"
        android:layout_marginTop="112dp"
        android:text="@string/L_TimePeriod"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/F90_Btn_Back"
        android:layout_width="176dp"
        android:layout_height="76dp"
        android:layout_marginTop="64dp"
        android:text="@string/L_Back"
        app:layout_constraintEnd_toEndOf="@+id/F90_Btn_TimePeriod"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/F90_Btn_TimePeriod"
        app:layout_constraintTop_toBottomOf="@+id/F90_Btn_TimePeriod" />
</androidx.constraintlayout.widget.ConstraintLayout>